                            "md5": "f863ce5b4051d68e9a3e626da998f0fc",
                            "bin": "eth"
                        },
                        "bin": "eth",
                        "commands": {
                            "sanity": {
                                "args": ["--version"],
                                "output": [ "eth", "1.3.0" ]
