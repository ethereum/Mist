
{
    "defaults": {
        "nodeType": "Geth"
    },
    "clients": {
        "eth": {
            "version": "1.3.0",
            "platforms": {
                "mac": {
                    "x64": {
                        "download": {
                            "url": "https://github.com/luclu/mist/releases/download/v1.0.0/eth-v1.0.0.tar.gz",
